<Window x:Class="FirstWPFLabs.AddStringWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FirstWPFLabs"
        mc:Ignorable="d"
        Title="AddStringWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="InputValue" HorizontalAlignment="Center" Margin="0,101,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="148" Height="22"/>
        <Button x:Name="EnterValueButton" HorizontalAlignment="Left" Margin="496,102,0,0" VerticalAlignment="Top" Width="50" Height="20" Content="add" Click="EnterValueButton_Click"/>
        <ListBox x:Name="MessageListBox" HorizontalAlignment="Left" Height="200" Margin="10,101,0,0" VerticalAlignment="Top" Width="294"/>
    </Grid>
</Window>
